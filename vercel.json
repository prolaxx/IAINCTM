{
    "version": 2,
    "builds": [
      {
        "src": "app/api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/api/$1"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "@openai_api_key",
      "OTHER_ENV_VARIABLE": "@other_env_variable"
    }
  }
  